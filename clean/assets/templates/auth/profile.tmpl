<link rel="stylesheet" media="all" href="{{static_url "/auth/styles/qor_auth.css"}}" />
<div class="container qor-auth">
    <div class="qor-auth-box">
        <h1>{{t (.I18n "my_profile")}}</h1>

    {{$flashes := .Flashes}}
    {{if $flashes}}
        <ul>
        {{range $flash := $flashes}}
            <li>{{$flash.Message}}</li>
        {{end}}
        </ul>
    {{end}}

    {{include "auth/partial/change_password"}}
</div>
</div>